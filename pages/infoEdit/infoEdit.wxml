<!--pages/userInfo/userInfo.wxml-->
<view class="">
<form bindsubmit="formSubmit">
<view class="editInfoListPanel">
    <scroll-view scroll-y="true" class="">
                   <view class="editInfoList">
                        <text>昵称</text><input name="nickname"  value="{{nickname}}" />
                    </view>
                    <view class="editInfoList">
                        <text>电话</text><input name="phone"  value="{{phone}}" />
                    </view>
                    <view class="editInfoList">
                        <text>地址</text>
                        <button bindtap="toMap" type="primary" style="margin：10px 0">点击快速找到地址</button>
                        <input name="address"  value="{{address}}"/>
                    </view>
    </scroll-view>
</view>
    <button class="btnOnbottom" form-type="submit">确定</button>
</form>
</view>
