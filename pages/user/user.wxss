.userHeader{
    background: #ffd344;
    text-align: center;
}

.userHeader>image{
    width: 100px;
    height: 100px;
    border-radius: 50px;
    margin: 30px 0 5px 0;
    background: #fff;
}

.userHeader>view{
    margin: 5px;
}

.userPageListPanel{
    width: 96%;
    margin: 30px auto 0 auto;
}

.userPageList{
    border-bottom: 1px solid #ccc;
    font-size: 20px;
    color: #333;
    line-height: 40px;
    vertical-align: middle;
}

.userListIcon{
    width: 30px;
    height: 30px;
    vertical-align: middle;
    display: inline-block;
    margin: 0 10px 0 0;
}

